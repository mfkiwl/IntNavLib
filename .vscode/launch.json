    "version": "0.2.0",
    "configurations": [

        {
            "name": "(gdb) Launch nav_sim",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/apps/nav_sim/build/nav_sim",
            "args": ["${workspaceFolder}/apps/nav_sim/config/closed_loop.json"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/apps/nav_sim/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },

        {
            "name": "(gdb) Launch gtest integrated",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/test_integrated",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },

        {
            "name": "(gdb) Launch gtest ins ideal",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/test_ins_ideal",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },

        {
        "name": "(gdb) launch ros2 node",
        "request": "launch",
        "type": "cppdbg",
        "miDebuggerServerAddress": "localhost:3000",
        "cwd": "/",
        "program": "${workspaceFolder}/apps/ros2_lc_ins_gnss_ecef/install/ros2_lc_ins_gnss_ecef/lib/ros2_lc_ins_gnss_ecef/ros2_lc_ins_gnss_ecef",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Set Disassembly Flavor to Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    }]
}