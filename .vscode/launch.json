    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch ins",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/apps/ins_ecef/build/ins_ecef",
            "args": ["${workspaceFolder}/data/Profile_3.csv"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
